    </div>\r\n        <ul style={{ display: match.isExact ? \"block\" : \"none\" }}>\r\n          {listCategory.map((catagory, index) => {\r\n            return (\r\n              <CategoryItem item={{ ...catagory, ...props }} key={index} />\r\n            );\r\n          })}\r\n        </ul>\r\n      </div>\r\n\r\n      <div className=\"Menu\">\r\n        <ul>\r\n          {listMenu.map((item, index) => {\r\n            return <MenuItem item={item} key={index} />;\r\n          })}\r\n        </ul>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nfunction MenuItem({ item }) {\r\n  const match = useRouteMatch({ path: item.path });\r\n  return (\r\n    <Link\r\n      to={item.path}\r\n      exact={item && item.exact ? item.exact.toString() : \"false\"}\r\n      className=\"MenuLink\"\r\n    >\r\n      <li className={match && match.isExact ? \"Active\" : \"\"}>{item.label}</li>\r\n    </Link>\r\n  );\r\n}\r\n\r\nfunction CategoryItem({ item }) {\r\n  const { label, subCategory, selectedCategory } = item;\r\n  function handlSelectedItem(categorySeleted) {\r\n    if (selectedCategory && subCategory.length <= 0) {\r\n      selectedCategory(categorySeleted);\r\n    }\r\n  }\r\n  return (\r\n    <li onClick={() => handlSelectedItem(item)}>\r\n      {label}\r\n      <FontAwesomeIcon\r\n        icon={faAngleRight}\r\n        className={\r\n          subCategory && subCategory.length > 0\r\n            ? \"IconRight\"\r\n            : \"IconRightHidden\"\r\n        }\r\n      />\r\n      {subCategory && subCategory.length > 0 ? (\r\n        <SubCategoryItem item={{ subCategory, selectedCategory }} />\r\n      ) : (\r\n        \"\"\r\n      )}\r\n    </li>\r\n  );\r\n}\r\n\r\nfunction SubCategoryItem({ item }) {\r\n  const { label, subCategory, selectedCategory } = item;\r\n  return (\r\n    <div className=\"SubMenu\">\r\n      {subCategory.map((parentSubcategory, ind) => {\r\n        return (\r\n          <div className=\"ContentSubMenu\" key={ind}>\r\n            <h5>{parentSubcategory.label}</h5>\r\n            <ul>\r\n              {parentSubcategory.subCategory.map((subItem, index) => {\r\n                return (\r\n                  <li key={index} onClick={() => selectedCategory(subItem)}>\r\n                    {subItem.label}\r\n                  </li>\r\n                );\r\n              })}\r\n            </ul>\r\n          </div>\r\n        );\r\n      })}\r\n    </div>\r\n  );\r\n}\r\nexport default NavigateMenu;\r\n","C:\\Users\\qnguyen77\\Desktop\\reactjs\\study-react\\src\\screens\\product\\Product.jsx",["135"],"C:\\Users\\qnguyen77\\Desktop\\reactjs\\study-react\\src\\screens\\about\\About.jsx",["136"],"import React from \"react\";\r\nimport PropTypes from \"prop-types\";\r\n\r\nAbout.propTypes = {};\r\n\r\nfunction About(props) {\r\n  return <div>About</div>;\r\n}\r\n\r\nexport default About;\r\n","C:\\Users\\qnguyen77\\Desktop\\reactjs\\study-react\\src\\screens\\contact\\Contact.jsx",["137"],"import React from \"react\";\r\nimport PropTypes from \"prop-types\";\r\n\r\nContact.propTypes = {};\r\n\r\nContact.defaultProps = {};\r\n\r\nfunction Contact(props) {\r\n  return <div>Contact</div>;\r\n}\r\n\r\nexport default Contact;\r\n","C:\\Users\\qnguyen77\\Desktop\\reactjs\\study-react\\src\\screens\\home\\Home.jsx",["138"],"C:\\Users\\qnguyen77\\Desktop\\reactjs\\study-react\\src\\screens\\news\\News.jsx",["139","140"],"import React from \"react\";\r\nimport PropTypes from \"prop-types\";\r\nimport VerticalMenu from \"../../component/VerticalMenu/VerticalMenu\";\r\nimport FeaturedNews from \"../../component/FeaturedNews/FeaturedNews\";\r\nimport NavigateQuickly from \"../../component/NavigateQuickly/NavigateQuickly\";\r\nimport Header from \"../../component/Header/Header\";\r\nimport CardNews from \"../../component/CardNews/CardNews\";\r\n\r\nimport \"./News.scss\";\r\nNews.propTypes = {};\r\n\r\nfunction News(props) {\r\n  function selectedCategory(item) {}\r\n\r\n  function selectedNews(item) {}\r\n\r\n  function get